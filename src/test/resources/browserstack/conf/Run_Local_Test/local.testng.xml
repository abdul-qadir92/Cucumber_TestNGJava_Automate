<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="methods">
    <parameter name="config" value="Run_Local_Test/local.conf.json"/>
    <listeners>
        <listener class-name="com.qa.util.SuiteAlterer"/>
    </listeners>
    <test verbose="2" name="LocalTesT">
        <parameter name="environment" value="env"/>
        <classes>
            <class name="Runner.CucumberTest"/>
        </classes>
    </test>
</suite>